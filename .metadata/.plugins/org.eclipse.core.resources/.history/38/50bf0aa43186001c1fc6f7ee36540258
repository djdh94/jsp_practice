	List<Integer> lotto = new ArrayList<>();
	int num=(int)(Math.random()*45)+1;
	while(true){
		if(!lotto.contains(num)){
			lotto.add(num);
		}
		else if(lotto.size()==6){
			break;
		}
	}
	
